{
  "grammars": [
    {
      "name": "familymarkup",
      "camelcase": "FamilyMarkup",
      "scope": "source.fml",
      "path": ".",
      "file-types": [
        "fml",
        "family"
      ],
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "1.8.3",
    "license": "BSD 4-Clause",
    "description": "Tree sitter for FamilyMarkup",
    "authors": [
      {
        "name": "Sergii Kliuchnyk"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-familymarkup"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
